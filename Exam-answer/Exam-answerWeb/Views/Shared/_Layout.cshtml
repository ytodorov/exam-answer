@model Exam_answerWeb.Models.BaseViewModel
<!doctype html>
<html amp lang="en">
<head>
    <meta charset="utf-8">
    <title>@Model?.PageTitle</title>
    <link rel="canonical" href="@Model?.PageCanonicalUrl">
    <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">
    <meta name="description" content="@Model?.PageDescription" />
    <meta property="fb:app_id" content="438289293607244" />
    <meta property="og:url" content="@(Model?.PageCanonicalUrl)" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="@(Model?.PageTitle)" />
    <meta property="og:description" content="@(Model?.PageDescription)" />
    @if (ViewData["imageUrl"] != null)
    {
        <meta property="og:image" content="https://www.exam-answer.com/@(ViewData["imageUrl"])" />
        <meta property="og:image:alt" content="@(Model?.PageTitle)" />
    }
    <link href="https://fonts.googleapis.com/css?family=Inconsolata:700" rel="stylesheet">
    <partial name="_BoilerplateCssPartial.cshtml" />
    <partial name="_AmpCustomCssPartial.cshtml" />
    <script async="" src="https://cdn.ampproject.org/v0.js"></script>
    <script async custom-element="amp-accordion" src="https://cdn.ampproject.org/v0/amp-accordion-0.1.js"></script>
    <script custom-element="amp-sidebar" src="https://cdn.ampproject.org/v0/amp-sidebar-0.1.js" async=""></script>
    <script custom-element="amp-bind" src="https://cdn.ampproject.org/v0/amp-bind-0.1.js" async=""></script>
    <script async custom-element="amp-addthis" src="https://cdn.ampproject.org/v0/amp-addthis-0.1.js"></script>
    <script async custom-element="amp-analytics" src="https://cdn.ampproject.org/v0/amp-analytics-0.1.js"></script>
    <script async custom-element="amp-iframe" src="https://cdn.ampproject.org/v0/amp-iframe-0.1.js"></script>
    <script async custom-element="amp-ad" src="https://cdn.ampproject.org/v0/amp-ad-0.1.js"></script>
</head>
<body>
    @if (this?.Context?.Request?.Host.ToString()?.Contains("localhost", StringComparison.InvariantCultureIgnoreCase) == false)
    {
        <amp-analytics config="https://www.googletagmanager.com/amp.json?id=GTM-MFQNVWQ&gtm.url=SOURCE_URL" data-credentials="include"></amp-analytics>
    }

    <header class="a-headerbar fixed flex justify-start items-center top-0 left-0 right-0 pl2 pr4 ">
        <div role="button" aria-label="open sidebar" on="tap:header-sidebar.toggle" tabindex="0" class="a-navbar-trigger md-hide lg-hide pr2  ">
            ☰
        </div>
        <a href="@($"{Model?.PageBaseCanonicalUrl}")" class="text-decoration-none inline-block mx-auto a-headerbar-home-link  ">
            <div class="a-headerbar-title mx-auto ">Exam-Answer</div>
        </a>
        <nav class="a-headerbar-nav a-nav  xs-hide sm-hide">
            <ul class="list-reset center m0 p0 flex justify-center nowrap">
                <li class="a-nav-item "><a href="@($"{Model?.PageBaseCanonicalUrl}/about")" class="text-decoration-none block">About</a></li>
                <li class="a-nav-item "><a href="@($"{Model?.PageBaseCanonicalUrl}/contact")" class="text-decoration-none block">Contact</a></li>
            </ul>
        </nav>
    </header>

    <amp-sidebar id="header-sidebar" class="a-sidebar px3  md-hide lg-hide " layout="nodisplay">
        <div class="flex justify-start items-center a-sidebar-header">
            <div role="button" aria-label="close sidebar" on="tap:header-sidebar.toggle" tabindex="0" class="a-navbar-trigger items-start">✕</div>
        </div>
        <nav class="a-sidebar-nav a-nav">
            <ul class="list-reset m0 p0 a-label">
                <li class="a-nav-item a-nav-dropdown relative ">
                    <amp-accordion layout="container" disable-session-states="" class="a-dropdown">
                        <section expanded="">
                            <header>Microsoft</header>
                            <ul class="a-dropdown-items list-reset m0 p0">
                                <li class="a-dropdown-item"><a href="@($"{Model?.PageBaseCanonicalUrl}/microsoft/az-100")" class="text-decoration-none">AZ-100</a></li>
                                <li class="a-dropdown-item"><a href="@($"{Model?.PageBaseCanonicalUrl}/microsoft/az-900")" class="text-decoration-none">AZ-900</a></li>
                            </ul>
                        </section>
                    </amp-accordion>
                </li>
                <li class="a-nav-item a-nav-dropdown relative ">
                    <amp-accordion layout="container" disable-session-states="" class="a-dropdown">
                        <section expanded="">
                            <header>Salesforce</header>
                            <ul class="a-dropdown-items list-reset m0 p0">
                                <li class="a-dropdown-item"><a href="@($"{Model?.PageBaseCanonicalUrl}/salesforce/crt-251")" class="text-decoration-none">CRT-251</a></li>
                                <li class="a-dropdown-item"><a href="@($"{Model?.PageBaseCanonicalUrl}/salesforce/salesforce-certified-field-service-lightning-consultant")" class="text-decoration-none">Salesforce Certified Field Service Lightning Consultant</a></li>
                            </ul>
                        </section>
                    </amp-accordion>
                </li>
                <li class="a-nav-item l-s-sidebar-nav-item a-title-sm bold"><a class="a-nav-link" href="@($"{Model?.PageBaseCanonicalUrl}/about")">About</a></li>
                <li class="a-nav-item l-s-sidebar-nav-item a-title-sm bold"><a class="a-nav-link" href="@($"{Model?.PageBaseCanonicalUrl}/contact")">Contact</a></li>

            </ul>
        </nav>   
    </amp-sidebar>
    <amp-ad width="100vw" height="320"
            type="adsense"
            data-ad-client="ca-pub-2519209558650417"
            data-ad-slot="6605505176"
            data-auto-format="rspv"
            data-full-width="">
        <div overflow=""></div>
    </amp-ad>
    @RenderBody()
    <footer class="a-footer flex flex-column items-center mx-auto max-width-3 ">
        <ul class="a-social-follow list-reset flex justify-around items-center flex-wrap m0 mb4">
            <li>
                <a href="https://www.facebook.com/ExamAnswerCom" target="_blank" class="inline-block p1" aria-label="https://www.facebook.com/ExamAnswerCom">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="23.6" viewBox="0 0 56 55">
                        @*BAD SEO<title>https://www.facebook.com/ExamAnswerCom</title>*@
                        <path d="M47.5 43c0 1.2-.9 2.1-2.1 2.1h-10V30h5.1l.8-5.9h-5.9v-3.7c0-1.7.5-2.9 3-2.9h3.1v-5.3c-.6 0-2.4-.2-4.6-.2-4.5 0-7.5 2.7-7.5 7.8v4.3h-5.1V30h5.1v15.1H10.7c-1.2 0-2.2-.9-2.2-2.1V8.3c0-1.2 1-2.2 2.2-2.2h34.7c1.2 0 2.1 1 2.1 2.2V43" class="a-icon a-icon-fb"></path>
                    </svg>
                </a>
            </li>
            <li>
                <a href="mailto:contact@@exam-answer.com?Subject=Hello%20Exam-Answer" target="_blank" class="inline-block p1" aria-label="Send mail to contact@@exam-answer.com">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="18.4" viewBox="0 0 56 43">
                        @*BAD SEO<title>Send mail to contact@@exam-answer.com</title>*@
                        <path d="M10.5 6.4C9.1 6.4 8 7.5 8 8.9v21.3c0 1.3 1.1 2.5 2.5 2.5h34.9c1.4 0 2.5-1.2 2.5-2.5V8.9c0-1.4-1.1-2.5-2.5-2.5H10.5zm2.1 2.5h30.7L27.9 22.3 12.6 8.9zm-2.1 1.4l16.6 14.6c.5.4 1.2.4 1.7 0l16.6-14.6v19.9H10.5V10.3z" class="a-icon a-icon-email">
                        </path>
                    </svg>
                </a>
            </li>
        </ul>
        <nav class="a-footer-nav">
            <ul class="list-reset flex flex-wrap mb3">
                <li class="px1"><a class="text-decoration-none a-label" href="@($"{Model?.PageBaseCanonicalUrl}/about")">About</a></li>
                <li class="px1"><a class="text-decoration-none a-label" href="@($"{Model?.PageBaseCanonicalUrl}/contact")">Contact</a></li>
            </ul>
        </nav>
        <small>
            © Exam Answer, @(DateTime.UtcNow.Year) •
            <a class="text-decoration-none" href="@($"{Model?.PageBaseCanonicalUrl}/privacy")">Privacy Policy</a>
        </small>
        <div class="center">

            <amp-addthis width="320" height="92" data-pub-id="ra-571b2acff4b44bbf" data-widget-id="vgua" data-widget-type="inline"></amp-addthis>
        </div>
    </footer>
    <amp-iframe width="auto"
                height="300"
                sandbox="allow-scripts"
                layout="fixed-height"
                src="@(Model?.PageBaseCanonicalUrl)/trustlogo">
        <amp-img layout="fill" src="@(Model?.PageBaseCanonicalUrl)/whiteimage.png" placeholder></amp-img>
    </amp-iframe>
    @*<amp-auto-ads type="adsense"
                      data-ad-client="ca-pub-2519209558650417">
        </amp-auto-ads>*@


    @Html.Raw(Model?.PageMicrodata)
</body>
</html>